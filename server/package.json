{
  "name": "esus-audit-ai-server",
  "version": "1.0.0",
  "type": "module",
  "description": "Express backend for Esus Audit AI with Supabase integration",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "dev": "NODE_ENV=development nodemon server.js",
    "prod": "NODE_ENV=production node server.js",
    "test": "jest",
    "validate-env": "node -e \"import('./utils/envValidator.js').then(m => m.validateEnvOrExit())\"",
    "lint": "eslint . --quiet",
    "build": "NODE_ENV=production npm run validate-env",
    "clean": "rm -rf logs/*.log temp/* uploads/*",
    "health": "curl -f http://localhost:3001/health || exit 1"
  },
  "dependencies": {
    "@azure/ai-form-recognizer": "^5.1.0",
    "@azure/core-auth": "^1.9.0",
    "@azure/openai": "^2.0.0",
    "@azure/search-documents": "^12.1.0",
    "@azure/service-bus": "^7.9.3",
    "@supabase/supabase-js": "^2.50.0",
    "applicationinsights": "^2.9.1",
    "axios": "^1.9.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-rate-limit": "^6.11.2",
    "express-validator": "^7.2.1",
    "formidable": "^3.5.4",
    "helmet": "^7.2.0",
    "html-pdf": "^3.0.1",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.1",
    "mammoth": "^1.6.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "openai": "^4.104.0",
    "pdf-parse": "^1.1.1",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
